{
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
    "headers": [
        {
            "source": "/api/cards/(.*)",
            "headers": [
                {
                    "key": "cache-control",
                    "value": "public, max-age=0, s-maxage=14400"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "https://github.com/vn7n24fzkq/github-profile-summary-cards"
        }
    ]
}
